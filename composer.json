{
    "name": "iono/console-kernel",
    "description": "php cli application package(kernel)",
    "minimum-stability": "stable",
    "license": "MIT",
    "keywords": ["console", "php", "cli"],
    "authors": [
        {
            "name": "yuuki.takezawa",
            "email": "yuuki.takezawa@comnect.jp.net"
        }
    ],
    "require": {
        "php": ">=5.4",
        "ext-tokenizer": "*",
        "symfony/console": "2.*",
        "kevinlebrun/colors.php": "0.*",
        "andrewsville/php-token-reflection": "*",
        "illuminate/config": "4.2.*",
	    "illuminate/cache": "4.2.*",
        "illuminate/container": "4.2.*",
        "illuminate/database": "4.2.*",
        "predis/predis": "0.*",
        "swiftmailer/swiftmailer": "5.*",
        "monolog/monolog": "1.*",
        "doctrine/annotations": "1.*",
	    "nesbot/carbon": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "4.*",
        "mockery/mockery": "*"
    },
    "autoload": {
        "psr-4": {
            "Iono\\Console\\": "src/"
        }
    }
}