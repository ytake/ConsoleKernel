{
    "name": "iono/console-kernel",
    "description": "php cli application package(kernel)",
    "minimum-stability": "stable",
    "license": "MIT",
    "keywords": ["console", "php", "cli"],
    "authors": [
        {
            "name": "yuuki.takezawa",
            "email": "yuuki.takezawa@comnect.jp.net"
        }
    ],
    "require": {
        "php": ">=5.4",
        "ext-tokenizer": "*",
        "symfony/console": "2.*",
        "kevinlebrun/colors.php": "0.*",
        "andrewsville/php-token-reflection": "*",
        "illuminate/config": "4.2.*",
        "illuminate/container": "4.2.*",
        "predis/predis": "1.*",
        "doctrine/cache": "1.*",
        "doctrine/dbal": "2.*",
        "swiftmailer/swiftmailer": "5.*",
        "monolog/monolog": "1.*",
        "nesbot/carbon": "*"
    },
    "require-dev": {
        "symfony/framework-bundle": "2.*",
        "symfony/console": "2.*",
        "phpunit/phpunit": "4.*",
        "mockery/mockery": "0.*",
        "satooshi/php-coveralls": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "Iono\\Console\\": "src/"
        }
    },
    "autoload-dev": {
        "files": [
            "tests/TestCase.php"
        ]
    },
    "suggest": {
        "ext-riak": "Riak database PHP extension",
        "ext-voltdb": "VoltDB database PHP extension",
        "ext-pcntl": ""
    }
}
